@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Index";
}
<partial name="_Notification">
<div class="py-3 d-flex justify-content-between">
    <h3> User Management </h3>

    <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary m-2">Create New User </a>
    </div>
<div class="card">
    <div class="card-header ">
        <div>
            <h6>View Users</h6>
        </div>
    </div>
    <div class="card-body">

       
            <table id="myTable" class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone Number</th>
                        <th>Company</th>
                        <th>Role</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
    
    </div>
</div>

@section scripts{
        <script src="~/js/User.js"></script>
}
